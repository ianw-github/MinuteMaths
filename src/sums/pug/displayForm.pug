
-var tab=data.table;
if tab
    h2.test Enter missing values
    form
    each row,index in data.table
        div.row(id='question-'+index, class="row-"+index)
            input.num1(type='text', name='num1', value='' + row[0], id='q'+index+'-1')
            span.times #{data.operator || 'x'}
            input.num2(type='text', name='num2', value='' + row[1], id='q'+index+'-2')
            span.equals =
            input.res(type='text', name='res', value='' + row[2], id='q'+index+'-3')

